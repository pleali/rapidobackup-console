{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "storageDir": ".openapi-generator",
    "generators": {
      "typescript-client": {
        "generatorName": "typescript-axios",
        "inputSpec": "src/main/resources/openapi/api.yml",
        "output": "src/main/webapp/src/lib/generated",
        "additionalProperties": {
          "npmName": "@rapidobackup/console-api-client",
          "npmVersion": "1.0.0",
          "supportsES6": "true",
          "withInterfaces": "true",
          "modelPropertyNaming": "camelCase",
          "enumPropertyNaming": "UPPERCASE"
        },
        "typeMappings": {
          "DateTime": "string"
        }
      }
    }
  }
}